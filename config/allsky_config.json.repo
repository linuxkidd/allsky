    "target_image_heaight_px": { "name":"Height", "value": 1080, "description":"Desired Image Height in Pixels", "default": 1080, "type":"uint", "unit":"pixels" }
  },
  "crop": {
    "h1": { "type":"header", "name":"Image Crop"},
    "enable": { "name":"Enable", "value": false, "description":"Useful to avoid distortion on edges of fisheye lens photos", "default": false, "type":"bool"},
