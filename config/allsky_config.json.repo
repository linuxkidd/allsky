    "enable": { "name":"Enable", "value": true, "description":"", "default": true, "type":"bool" },
    "target_width_px": { "name":"Thumbnail Width", "value": 100, "description":"", "default": 100, "type":"uint", "unit":"pixels" },
    "target_height_px": { "name":"Thumbnail Height", "value": 75, "description":"", "default": 75, "type":"uint", "unit":"pixels" }
  },
  "badremoval": {
    "h1": { "type":"header", "name":"Bad Exposure Removal"},
    "enable": { "name":"Enable", "value": false, "description":"isn't necessary unless your system sometimes produces corrupt images.", "default": false, "type":"bool"},
    "threshold_low_pct": { "name":"Low Threshold", "value": 1, "description":"Images below this mean brightness will be removed.", "default": 1, "type":"uint"},
    "threshold_high_pct": { "name":"High Threshold", "value": 90, "description":"Images above this mean brightness will be removed.", "default": 90, "type":"uint"}
  },
  "keogram": {
    "h1": { "type":"header", "name":"Keogram"},
    "enable": { "name":"Generate", "value": true, "description":"Generate Keogram at the end of the Night.", "default": true, "type":"bool"},
    "upload": { "name":"Upload", "value": false, "description":"Upload the Keogram ( depends on upload settings below )", "default": false, "type":"bool"}
  },
