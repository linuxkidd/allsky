    "crop_offset_top_px": { "name":"Top Offset", "value": 100, "description":"Crop Offset from Top of the image.", "default": 100, "type":"uint", "unit":"pixels" }
  },
  "stretch": {
    "h1": { "type":"header", "name":"Image Auto-Stretch"},
    "enable": { "name":"Enable", "value": false, "description":"Enable Auto-Stretch of the night exposures.", "default": false, "type":"bool" },
    "amount": { "name":"Amount", "value": 10, "description":"Amount of Stretch to apply", "default": 10, "type":"uint" },
    "midpoint_pct": { "name":"Midpoint", "value": 10, "description":"Mid-point of stretch (from left of histogram)", "default": 10, "type":"uint", "unit":"percent" }
  },
  "thumbnail": {
